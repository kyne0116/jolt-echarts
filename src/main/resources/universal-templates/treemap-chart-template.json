{
  "chartMeta": {
    "title": "${chart_title}",
    "subtitle": "${chart_subtitle}",
    "chartType": "${chart_type}",
    "templateCategory": "TREE",
    "description": "${chart_description}"
  },
  "dataStructure": {
    "series": [
      {
        "seriesName": "${series_name}",
        "seriesType": "${tree_type}",
        "data": "${tree_data}",
        "roam": "${enable_roam}",
        "nodeClick": "${node_click_behavior}",
        "zoomToNodeRatio": "${zoom_to_node_ratio}",
        "levels": "${tree_levels_config}",
        "silent": "${silent_mode}",
        "animationDurationUpdate": "${animation_duration_update}",
        "animationEasingUpdate": "${animation_easing_update}",
        "breadcrumb": {
          "show": "${show_breadcrumb}",
          "height": "${breadcrumb_height}",
          "emptyItemWidth": "${breadcrumb_empty_width}",
          "itemStyle": "${breadcrumb_item_style}"
        },
        "label": {
          "show": "${show_label}",
          "position": "${label_position}",
          "formatter": "${label_formatter}",
          "fontSize": "${label_font_size}",
          "color": "${label_color}",
          "fontWeight": "${label_font_weight}",
          "align": "${label_align}",
          "verticalAlign": "${label_vertical_align}"
        },
        "upperLabel": {
          "show": "${show_upper_label}",
          "position": "${upper_label_position}",
          "formatter": "${upper_label_formatter}",
          "fontSize": "${upper_label_font_size}",
          "color": "${upper_label_color}"
        },
        "itemStyle": {
          "borderRadius": "${border_radius}",
          "borderColor": "${border_color}",
          "borderWidth": "${border_width}",
          "gapWidth": "${gap_width}",
          "borderColorSaturation": "${border_color_saturation}"
        },
        "emphasis": {
          "focus": "${emphasis_focus}",
          "itemStyle": {
            "borderColor": "${emphasis_border_color}",
            "borderWidth": "${emphasis_border_width}"
          },
          "label": {
            "show": "${emphasis_show_label}",
            "fontSize": "${emphasis_label_font_size}",
            "color": "${emphasis_label_color}"
          }
        }
      }
    ]
  },
  "styleConfig": {
    "showLegend": "${show_legend}",
    "legendPosition": "${legend_position}",
    "showTooltip": "${show_tooltip}",
    "tooltipTrigger": "${tooltip_trigger}",
    "tooltipFormatter": "${tooltip_formatter}",
    "colorPalette": "${color_palette}",
    "animation": "${enable_animation}",
    "animationDuration": "${animation_duration}",
    "animationEasing": "${animation_easing}"
  },
  "interactionConfig": {
    "toolboxConfig": {
      "show": "${show_toolbox}",
      "features": "${toolbox_features}"
    },
    "brushConfig": {
      "show": "${enable_brush}"
    }
  },
  "treemapSpecialConfig": {
    "squareRatio": "${square_ratio}",
    "leafDepth": "${leaf_depth}",
    "drillDownIcon": "${drill_down_icon}",
    "visualDimension": "${visual_dimension}",
    "visualMin": "${visual_min}",
    "visualMax": "${visual_max}",
    "colorAlpha": "${color_alpha}",
    "colorSaturation": "${color_saturation}",
    "visibleMin": "${visible_min}",
    "childrenVisibleMin": "${children_visible_min}"
  },
  "sunburstSpecialConfig": {
    "radius": "${sunburst_radius}",
    "center": "${sunburst_center}",
    "startAngle": "${start_angle}",
    "minAngle": "${min_angle}",
    "sort": "${sort_order}"
  }
}