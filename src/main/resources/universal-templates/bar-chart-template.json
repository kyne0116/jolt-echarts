{
  "chartMeta": {
    "title": "${chart_title}",
    "subtitle": "${chart_subtitle}",
    "chartType": "${chart_type}",
    "templateCategory": "CARTESIAN",
    "description": "${chart_description}"
  },
  "axisConfig": {
    "xAxis": {
      "type": "category",
      "boundaryGap": "${boundary_gap}",
      "name": "${x_axis_name}",
      "nameLocation": "${x_axis_name_location}",
      "axisTick": {
        "alignWithLabel": "${align_with_label}"
      }
    },
    "yAxis": {
      "type": "value",
      "name": "${y_axis_name}",
      "nameLocation": "${y_axis_name_location}",
      "min": "${y_axis_min}",
      "max": "${y_axis_max}"
    }
  },
  "dataStructure": {
    "categories": "${categories}",
    "series": [
      {
        "seriesName": "${series_1_name}",
        "values": "${series_1_data}",
        "seriesType": "bar",
        "stackGroup": "${stack_group}",
        "barWidth": "${bar_width}",
        "barMaxWidth": "${bar_max_width}",
        "barGap": "${bar_gap}",
        "barCategoryGap": "${bar_category_gap}",
        "itemStyle": {
          "color": "${series_1_color}",
          "borderRadius": "${border_radius}",
          "borderWidth": "${border_width}",
          "borderColor": "${border_color}"
        },
        "label": {
          "show": "${show_label}",
          "position": "${label_position}",
          "formatter": "${label_formatter}"
        }
      },
      {
        "seriesName": "${series_2_name}",
        "values": "${series_2_data}",
        "seriesType": "bar",
        "stackGroup": "${stack_group}",
        "barWidth": "${bar_width}",
        "barMaxWidth": "${bar_max_width}",
        "itemStyle": {
          "color": "${series_2_color}",
          "borderRadius": "${border_radius}",
          "borderWidth": "${border_width}",
          "borderColor": "${border_color}"
        },
        "label": {
          "show": "${show_label}",
          "position": "${label_position}",
          "formatter": "${label_formatter}"
        }
      },
      {
        "seriesName": "${series_3_name}",
        "values": "${series_3_data}",
        "seriesType": "bar",
        "stackGroup": "${stack_group}",
        "barWidth": "${bar_width}",
        "barMaxWidth": "${bar_max_width}",
        "itemStyle": {
          "color": "${series_3_color}",
          "borderRadius": "${border_radius}",
          "borderWidth": "${border_width}",
          "borderColor": "${border_color}"
        },
        "label": {
          "show": "${show_label}",
          "position": "${label_position}",
          "formatter": "${label_formatter}"
        }
      },
      {
        "seriesName": "${series_4_name}",
        "values": "${series_4_data}",
        "seriesType": "bar",
        "stackGroup": "${stack_group}",
        "barWidth": "${bar_width}",
        "barMaxWidth": "${bar_max_width}",
        "itemStyle": {
          "color": "${series_4_color}",
          "borderRadius": "${border_radius}",
          "borderWidth": "${border_width}",
          "borderColor": "${border_color}"
        },
        "label": {
          "show": "${show_label}",
          "position": "${label_position}",
          "formatter": "${label_formatter}"
        }
      }
    ]
  },
  "styleConfig": {
    "showLegend": "${show_legend}",
    "legendPosition": "${legend_position}",
    "showTooltip": "${show_tooltip}",
    "tooltipTrigger": "${tooltip_trigger}",
    "showGrid": "${show_grid}",
    "gridConfig": {
      "left": "${grid_left}",
      "right": "${grid_right}",
      "top": "${grid_top}",
      "bottom": "${grid_bottom}",
      "containLabel": "${contain_label}"
    },
    "colorPalette": "${color_palette}",
    "animation": "${enable_animation}",
    "animationDuration": "${animation_duration}",
    "animationDelay": "${animation_delay}"
  },
  "interactionConfig": {
    "enableBrush": "${enable_brush}",
    "enableDataZoom": "${enable_datazoom}",
    "dataZoomConfig": {
      "type": "${datazoom_type}",
      "start": "${datazoom_start}",
      "end": "${datazoom_end}"
    },
    "toolboxConfig": {
      "show": "${show_toolbox}",
      "features": "${toolbox_features}"
    }
  },
  "specialConfig": {
    "orientation": "${chart_orientation}",
    "polar": "${enable_polar}"
  }
}